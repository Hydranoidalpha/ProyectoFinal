<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.snpp.proyectofinal.login.LoginController">
   <children>
      <JFXTextField fx:id="ci" focusColor="#209aeb" labelFloat="true" layoutX="121.0" layoutY="281.0" prefHeight="35.0" prefWidth="278.0" promptText="C.I. Nro" unFocusColor="#a19e9e">
         <font>
            <Font size="14.0" />
         </font>
      </JFXTextField>
      <JFXButton fx:id="aceptar" buttonType="RAISED" layoutX="461.0" layoutY="360.0" onAction="#onActiononAceptar" ripplerFill="#1e77b2" style="-fx-background-color: lightblue; -fx-border-radius: 100;" text="Aceptar" wrapText="true">
         <effect>
            <Shadow height="0.0" radius="0.0" width="0.0" />
         </effect>
      </JFXButton>
      <JFXButton fx:id="cancelar" buttonType="RAISED" layoutX="559.0" layoutY="360.0" onAction="#OnActiononCancelar" ripplerFill="#1e77b2" style="-fx-background-color: lightblue; -fx-border-radius: 100;" text="Cancelar" wrapText="true">
         <effect>
            <Shadow height="0.0" radius="0.0" width="0.0" />
         </effect>
      </JFXButton>
      <Label fx:id="config" layoutX="141.0" layoutY="418.0" onMouseClicked="#OnMouseClicked" text="Configuración de la cuenta" textFill="#206df2">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Label>
      <JFXPasswordField fx:id="contrasena" focusColor="#4981c6" labelFloat="true" layoutX="121.0" layoutY="347.0" prefHeight="26.0" prefWidth="278.0" promptText="Contraseña" unFocusColor="#726f6f" />
      <ImageView fx:id="imagen" fitHeight="142.0" fitWidth="544.0" layoutX="103.0" layoutY="40.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
